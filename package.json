{
  "name": "skupper-network-console",
  "version": "2.0.0",
  "description": "The Network Console for monitoring Skupper metrics and topology.",
  "repository": "git+https://github.com/skupperproject/skupper-console.git",
  "author": "The Skupper Team",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22 <2.0.0"
  },
  "scripts": {
    "build": "tsc && webpack --config webpack.config.mjs",
    "build:preview": "vite preview",
    "start": "vite --host",
    "test": "yarn test:watch --run",
    "test:watch": "USE_MOCK_SERVER=true vitest",
    "coverage": "yarn test --coverage",
    "cy:dev": "cypress open",
    "cy:run": "cypress run",
    "ci": "if [ -z \"$CYPRESS_BASE_URL\" ]; then start-test 3000 cy:run; else yarn cy:dev; fi",
    "lint": "TIMING=1 eslint --cache",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write '*/**/*.{ts,tsx,json,css}'",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@antv/g6": "5.0.45",
    "@nivo/sankey": "0.88.0",
    "@patternfly/patternfly": "6.1.0",
    "@patternfly/react-charts": "8.1.0",
    "@patternfly/react-core": "6.1.0",
    "@patternfly/react-icons": "6.1.0",
    "@patternfly/react-table": "6.1.0",
    "@patternfly/react-tokens": "6.1.0",
    "@tanstack/react-query": "5.71.5",
    "axios": "1.8.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "5.0.0",
    "react-router-dom": "7.4.1",
    "victory": "37.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.24.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/eslint-plugin": "^1.1.42",
    "circular-dependency-plugin": "^5.2.2",
    "commitizen": "^4.3.1",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "cypress": "^14.3.0",
    "cz-git": "^1.11.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "mini-css-extract-plugin": "^2.9.2",
    "miragejs": "^0.1.48",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6",
    "vitest": "^3.1.1",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
